{
  "name": "vod",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "npm run build:test && mocha test/dist/main.js",
    "test:watch": "nodemon -e ts,tsx --watch test --exec npm run test",
    "test:lint": "eslint 'src/**/*.ts' 'src/**/*.tsx'",
    "test:type-check": "tsc --noEmit --skipLibCheck",
    "build": "NODE_ENV=production parallel-webpack",
    "build:test": "webpack --config webpack.conf.d/test.js",
    "build:server": "NODE_ENV=production webpack --config webpack.conf.d/server.js",
    "build:client": "NODE_ENV=production webpack --config webpack.conf.d/client.js",
    "build:watch": "parallel-webpack --watch",
    "start": "ISOMORPHIC_ENV=server ts-node --files --pretty src/server.ts",
    "start:watch": "ISOMORPHIC_ENV=server nodemon -d 1 --ignore node_modules/ --watch dist --watch src/server.ts --exec npm run start",
    "start:dev": "npm-run-all --parallel build:watch start:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hexeo/vod.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hexeo/vod/issues"
  },
  "homepage": "https://github.com/hexeo/vod#readme",
  "dependencies": {
    "@azure/arm-mediaservices": "^5.0.1",
    "@azure/arm-storage": "^7.0.1",
    "@azure/ms-rest-browserauth": "^0.1.3",
    "@azure/storage-blob": "^10.3.0",
    "@hexeo/vue-typescript-jsx": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^1.1.0",
    "@typescript-eslint/parser": "^1.1.0",
    "azure-storage": "^2.10.2",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "eslint-plugin-react": "^7.12.4",
    "express": "^4.16.4",
    "minireset.css": "0.0.4",
    "node-fetch": "^2.3.0",
    "postcss-calc": "^7.0.1",
    "serve-favicon": "^2.5.0",
    "ts-node": "^8.0.1",
    "tslib": "^1.9.3",
    "typescript": "^3.2.4",
    "vue": "^2.5.22",
    "vue-class-component": "^6.3.2",
    "vue-property-decorator": "^7.3.0",
    "vue-router": "^3.0.2",
    "vue-server-renderer": "^2.5.22"
  },
  "devDependencies": {
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.1",
    "@types/node": "^10.12.18",
    "@types/node-fetch": "^2.1.4",
    "autoprefixer": "^9.4.6",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "cssnano-preset-default": "^4.0.6",
    "eslint": "^5.12.1",
    "eslint-config-google": "^0.12.0",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "parallel-webpack": "^2.3.0",
    "postcss-autoreset": "^2.0.2",
    "postcss-each": "^0.10.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.1",
    "postcss-nested": "^4.1.1",
    "postcss-prepend-imports": "^1.0.1",
    "postcss-reporter": "^6.0.1",
    "postcss-simple-vars": "^5.0.1",
    "ts-loader": "^5.3.3",
